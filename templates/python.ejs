    '<%= scheme %>://<%=server_name%><%=uri%>'<% if (locals.query_string) { %><%=',\n    '%>params='<%- query_string %>'<% } %><% if (locals.headers) { %><%=',\n    '%>headers=headers<% } %><% if (locals.pyBody) { %><%=',\n    '%>json=body<% } %>
